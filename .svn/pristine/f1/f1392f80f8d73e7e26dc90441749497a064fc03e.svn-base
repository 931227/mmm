  <!-- 品牌排行模板 -->
  <script type="text/html" id='rankTmp'>
    <div class="caption"><p>热门品牌排行</p></div>
    <ul class="list-group">
    {{each result as value}}
    <li class="list-group-item"><a href="#" data-id={{value.brandTitleId}}>{{value.brandTitle}}<i class="iconfont icon-weibiaoti35"></i></a></li>
    {{/each}}
    </ul>
  </script>
    <!-- 二级页面第一部分模板 -->
    <script type="text/html" id='oneTmp'> 
      <div class="one">
        <h3>平板电视哪个牌子好</h3>
        <ul>
        {{each result as value i}}
          <li><a href="#">
            <span>{{i+1}}</span>
            <h2>{{value.brandName}}</h2>
            <p>{{value.brandInfo}}</p>
            <i class="iconfont icon-xiangyou"></i>
          </a></li>
          {{/each}}
        </ul>
      </div>
    </script>
     <!-- 二级页面第二部分模板 -->
    <script type="text/html" id='twoTmp'>
      <div class="two">
        <h3>平板电视产品销量排行</h3>
        {{each result as value i}}
        <div class="media">
          <a class="pull-left" href="#">
            {{#value.productImg}}
          </a>
          <div class="media-body">
            <h4 class="media-heading">{{value.productName}}</h4>
            <p class="price">{{value.productPrice}}</p>
            <p class="info">{{value.productQuote}}  {{value.productCom}}</p>
          </div>
        </div>
        {{/each}}  
      </div>
    </script>
    <!-- 二级页面第三部分模板 -->
